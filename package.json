{
  "name": "jim-vsc-plugin-demo",
  "displayName": "jim-vsc-plugin-demo",
  "description": "",
  "publisher": "jim-elijah",
  "icon": "assets/img/logo.png",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onWebviewPanel:jim-vsc-movies",
    "onWebviewPanel:jim-vsc-news"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "jim-vsc",
          "title": "jim-vsc",
          "icon": "assets/img/logo.png"
        }
      ]
    },
    "views": {
      "jim-vsc": [
        {
          "type": "webview",
          "id": "movies",
          "name": "hot movies",
          "visibility": "visible"
        },
        {
          "type": "webview",
          "id": "news",
          "name": "hot news",
          "visibility": "visible"
        }
      ]
    },
    "commands": [
      {
        "command": "jim-vsc-plugin-demo.helloWorld",
        "title": "Hello World1"
      },
      {
        "command": "jim-vsc-call-cmd-on-other-ext",
        "title": "jim-vsc open browser"
      },
      {
        "command": "jim-vsc-plugin-demo.create",
        "title": "Pseudoterminal: Create"
      },
      {
        "command": "jim-vsc-plugin-demo.clear",
        "title": "Pseudoterminal: Clear"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --fix --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "glob": "^8.1.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.3",
    "mocha": "^10.2.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}
